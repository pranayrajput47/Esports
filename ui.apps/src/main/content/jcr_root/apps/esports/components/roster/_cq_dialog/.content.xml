<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Roster"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        granite:class="cmp-navigation__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <roster
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Roster"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <rosterHeading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Roster Heading"
                                                name="./rosterHeading"/>
                                            <rosterSubHeading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Roster Sub Heading"
                                                name="./rosterSubHeading"/>
                                            <rosterCTA
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Roster CTA Text"
                                                name="./rosterCTA"/>
                                            <rosterYear
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Roster Year"
                                                name="./rosterYear"/>
                                            <rosterTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Roster Title"
                                                name="./rosterTitle"/>
                                            <roster
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Roster">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./roster">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <rosterList
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                            composite="{Boolean}true"
                                                            fieldLabel="Roster List">
                                                            <field
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                name="./rosterList">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <imageLink
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                        fieldLabel="Image Link Path"
                                                                        name="./imageLink"/>
                                                                    <image
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                        fieldDescription="Make the image a link to another resource"
                                                                        fieldLabel="Image"
                                                                        name="./image"
                                                                        rootPath="/content/dam"/>
                                                                    <altText
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldLabel="Alternate Text"
                                                                        name="./altText"/>
                                                                </items>
                                                            </field>
                                                        </rosterList>
                                                    </items>
                                                </field>
                                            </roster>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </roster>
                    <modalConfig
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Modal Configurations"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <roster
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Modal List">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./modalList">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <modal
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                            composite="{Boolean}true"
                                                            fieldLabel="Modal">
                                                            <field
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                name="./modal">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <imageLink
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                        fieldLabel="Image Link Path"
                                                                        name="./imageLink"/>
                                                                    <image
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                        fieldDescription="Make the image a link to another resource"
                                                                        fieldLabel="Image"
                                                                        name="./image"
                                                                        rootPath="/content/dam"/>
                                                                    <altText
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldLabel="Alternate Text"
                                                                        name="./altText"/>
                                                                </items>
                                                            </field>
                                                        </modal>
                                                    </items>
                                                </field>
                                            </roster>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </modalConfig>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
