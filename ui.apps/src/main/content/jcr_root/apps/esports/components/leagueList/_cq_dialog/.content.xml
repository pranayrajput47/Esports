<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="League List Dialog"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        granite:class="cmp-list__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <listSettings
                        jcr:primaryType="nt:unstructured"
                        jcr:title="LeagueList"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <cardTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Card Title"
                                                name="./cardTitle"/>
                                            <cardSubTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Card Sub Title"
                                                name="./cardSubTitle"/>
                                            <ctaText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="CTA Text"
                                                name="./ctaText"/>
                                            <league
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Add News List Items">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./league">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <viewType
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                            fieldDescription="select the view type of component"
                                                            fieldLabel="View type"
                                                            name="./viewType">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <o1
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Academy"
                                                                    value="academy"/>
                                                                <o2
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="Champions"
                                                                    value="champion"/>
                                                            </items>
                                                        </viewType>
                                                        <heading
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Heading"
                                                            name="./heading"/>
                                                        <subHeading
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Sub Heading"
                                                            name="./subHeading"/>
                                                        <leagueList
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                            composite="{Boolean}true"
                                                            fieldLabel="Add League List">
                                                            <field
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                name="./leagueList">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <month
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldLabel="Month"
                                                                        name="./month"/>
                                                                    <date
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                                        fieldLabel="Date"
                                                                        name="./date"/>
                                                                    <week
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                                        fieldLabel="Week"
                                                                        name="./week"/>
                                                                    <time
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                                        fieldLabel="Time"
                                                                        name="./time"/>
                                                                </items>
                                                            </field>
                                                        </leagueList>
                                                    </items>
                                                </field>
                                            </league>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </listSettings>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
