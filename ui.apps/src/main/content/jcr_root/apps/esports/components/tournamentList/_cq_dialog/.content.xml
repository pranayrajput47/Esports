<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Tournament List Dialog"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <heading
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Heading"
                        name="./heading"/>
                    <subHeading
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Sub Heading"
                        name="./subHeading"/>
                    <toggle
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                        composite="{Boolean}true"
                        fieldLabel="Toggle">
                        <field
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            name="./toggle">
                            <items jcr:primaryType="nt:unstructured">
                                <toggleText
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Enter the Toggle Text"
                                    name="./toggleText"/>
                                <toggleLink
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldLabel="Toggle Link"
                                    name="./toggleLink"/>
                            </items>
                        </field>
                    </toggle>
                    <cards
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                        composite="{Boolean}true"
                        fieldLabel="Cards">
                        <field
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            name="./cards">
                            <items jcr:primaryType="nt:unstructured">
                                <toggleLink
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                    fieldLabel="Toggle Link"
                                    name="./toggleLink"
                                    rootPath="/content"/>
                                <toggleText
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Toggle Text"
                                    name="./toggleText"/>
                            </items>
                        </field>
                    </cards>
                    <cardLink
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                        fieldLabel="Card Link"
                        name="./cardLink"/>
                    <categoryHeading
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Category Heading"
                        name="./categoryHeading"/>
                    <categorySubHeading
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Category Sub Heading"
                        name="./categorySubHeading"/>
                    <dropdownList
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                        composite="{Boolean}true"
                        fieldLabel="Dropdown List">
                        <field
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            name="./dropdownList">
                            <items jcr:primaryType="nt:unstructured">
                                <listLink
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                    fieldLabel="List Link"
                                    name="./listLink"
                                    rootPath="/content"/>
                                <listText
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="List Text"
                                    name="./listText"/>
                            </items>
                        </field>
                    </dropdownList>
                    <customDropdownLink
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                        fieldLabel="Custom Dropdown Path"
                        name="./customDropdownLink"/>
                    <customDropdownHeading
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Custom Dropdown Heading"
                        name="./customDropdownHeading"/>
                    <customDropdownSubHeading
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Custom Dropdown Sub Heading"
                        name="./customDropdownSubHeading"/>
                    <menu
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                        composite="{Boolean}true"
                        fieldLabel="Menu">
                        <field
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            name="./menu">
                            <items jcr:primaryType="nt:unstructured">
                                <menuLink
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                    fieldLabel="Menu Link"
                                    name="./listLink"
                                    rootPath="/content"/>
                                <menuText
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Menu Text"
                                    name="./menuText"/>
                            </items>
                        </field>
                    </menu>
                    <contentList
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                        composite="{Boolean}true"
                        fieldLabel="Content List">
                        <field
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            name="./contentList">
                            <items jcr:primaryType="nt:unstructured">
                                <selecttion
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                    fieldDescription="Select image or text"
                                    fieldLabel="Selection"
                                    name="./selection">
                                    <items jcr:primaryType="nt:unstructured">
                                        <image
                                            jcr:primaryType="nt:unstructured"
                                            text="Image"
                                            value="image"/>
                                        <text
                                            jcr:primaryType="nt:unstructured"
                                            text="Text"
                                            value="text"/>
                                    </items>
                                </selecttion>
                                <contentHeading
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Content Heading"
                                    name="./contentHeading"
                                    rootPath="/content"/>
                                <contentSubHeading
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Content Sub Heading"
                                    name="./contentSubHeading"/>
                                <image
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    fieldDescription="Make the image a link to another resource"
                                    fieldLabel="Image Path"
                                    name="./image"
                                    rootPath="/content/dam"/>
                            </items>
                        </field>
                    </contentList>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
