<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="News List Dialog"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        granite:class="cmp-list__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <listSettings
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Configurations"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Title"
                                                fieldLabel="Title"
                                                name="./title"/>
                                            <browseText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Browse Text"
                                                fieldLabel="Browse Text"
                                                name="./browseText"/>
                                            <link
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Make a link to another resource"
                                                fieldLabel="Link "
                                                name="./link"
                                                rootPath="/content"/>
                                            <newsList
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Add News List Items">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./newsList">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <image
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldDescription="Make the image a link to another resource"
                                                            fieldLabel="Select the image"
                                                            name="./image"
                                                            rootPath="/content/dam"/>
                                                        <sponserText
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Sponser Text"
                                                            fieldLabel="Sponser Text"
                                                            name="./sponserText"/>
                                                        <logoImg
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldDescription="Make the logo a link to another resource"
                                                            fieldLabel="Select the logo"
                                                            name="./logoImg"
                                                            rootPath="/content/dam"/>
                                                        <logoLink
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldDescription="Logo Link"
                                                            fieldLabel="Logo Link"
                                                            name="./logoLink"
                                                            rootPath="/content"/>
                                                        <heading
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Heading"
                                                            fieldLabel="Heading"
                                                            name="./heading"/>
                                                        <linkHeader
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldDescription="Make a link to another resource"
                                                            fieldLabel="Heading Link"
                                                            name="./headingLink"
                                                            rootPath="/content"/>
                                                        <dateHeading
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Date Heading"
                                                            fieldLabel="Date Heading"
                                                            name="./dateHeading"/>
                                                        <writeBy
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Write By"
                                                            fieldLabel="Write By"
                                                            name="./writeBy"/>
                                                        <description
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Description"
                                                            fieldLabel="Description"
                                                            name="./description"/>
                                                        <btnLink
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldDescription="Button Link"
                                                            fieldLabel="Button Link"
                                                            name="./btnLink"
                                                            rootPath="/content"/>
                                                        <btnText
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Button Text"
                                                            fieldLabel="Button Text"
                                                            name="./btnText"/>
                                                    </items>
                                                </field>
                                            </newsList>
                                            <newsLinkList
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="Add News List Items">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./newsLinkList">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <count
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Count"
                                                            name="./conut"/>
                                                        <countLink
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldDescription="Make a link to another resource"
                                                            fieldLabel="Link "
                                                            name="./link"
                                                            rootPath="/content"/>
                                                        <date
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Date"
                                                            name="./date"/>
                                                    </items>
                                                </field>
                                            </newsLinkList>
                                            <linkPrevies
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Previous Link "
                                                fieldLabel="Previous Link "
                                                name="./linkPrevies"
                                                rootPath="/content"/>
                                            <linkNext
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Next Link "
                                                fieldLabel="Next Link "
                                                name="./linkNext"
                                                rootPath="/content"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </listSettings>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
